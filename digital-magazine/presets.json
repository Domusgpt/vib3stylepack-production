{
  "visualStyles": {
    "holo-dark-background": {
      "description": "Global holographic dark background for the magazine (WebGL).",
      "parameters": {
        "shader": "holoBackground", // Identifies this preset for WebGL path in VIB34D.js
        "color": [0.02, 0.02, 0.05, 1.0], // u_baseColor
        "timeScale": 0.1, // Multiplier for u_time (shader's internal 0.1 factor removed)
        "intensity": 0.25, // u_intensity
        "noiseAmount": 0.07, // u_noiseAmount
        // Conceptual/unused by current simple shader, but kept for future ideas:
        // "glitchFrequency": 0.02,
        // "particleSystem": "subtle-stars",
        // "shape": "hypercube", // Still useful conceptually or for other logic
        "renderAsBackground": true // Still useful conceptually
      }
    },
    "glass-panel-primary": {
      "description": "Primary glassmorphic panel style for content cards/sections.",
      "parameters": {
        "color": [0.0, 0.0, 0.0, 0.0], /* Fully transparent base, CSS handles panel color */
        "shape": "quad",
        "intensity": 0.0, /* No base intensity, rely on interaction */
        "internalPattern": "animated-noise", /* VIB3 provides subtle internal animation */
        "noiseDensity": 0.03,
        "noiseSpeed": 0.05,
        "edgeHighlightColor": [0.2, 0.5, 0.8, 0.0], /* Initially transparent edge */
        "edgeHighlightWidth": 2.0, /* Conceptual pixel width for edge */
        "isGlassPanel": true
      }
    },
    "glass-panel-accent": {
      "description": "Accent glassmorphic panel, perhaps for featured items.",
      "parameters": {
        "color": [0.0, 0.0, 0.0, 0.0],
        "shape": "quad",
        "intensity": 0.0,
        "internalPattern": "subtle-moire",
        "moireFrequency": 0.1,
        "moireSpeed": 0.02,
        "edgeHighlightColor": [0.5, 0.2, 0.5, 0.0], /* Initially transparent accent edge */
        "edgeHighlightWidth": 2.5,
        "isGlassPanel": true
      }
    },
    "category-ema-visual": {
      "description": "Visual theme for EMA Reports category.",
      "parameters": {
        "shape": "tetrahedron",
        "baseColor": [0.3, 0.7, 0.9, 0.15], /* Slightly more opaque */
        "speed": 0.08, /* Slower, more thoughtful */
        "intensity": 0.35,
        "particleDensity": 0.2 /* Conceptual */
      }
    },
    "category-ai-visual": {
      "description": "Visual theme for AI Vibes category.",
      "parameters": {
        "shape": "sphere",
        "baseColor": [0.7, 0.3, 0.9, 0.15],
        "speed": 0.18, /* Faster, more dynamic */
        "intensity": 0.55,
        "flowFieldStrength": 0.3 /* Conceptual */
      }
    },
    "category-code-visual": {
      "description": "Visual theme for Code Liberation category.",
      "parameters": {
        "shape": "torus",
        "baseColor": [0.3, 0.9, 0.7, 0.15],
        "speed": 0.1,
        "intensity": 0.4,
        "lineThickness": 0.8 /* Conceptual */
      }
    },
    "category-future-visual": {
      "description": "Visual theme for Future Forge category.",
      "parameters": {
        "shape": "klein",
        "baseColor": [0.9, 0.7, 0.3, 0.12],
        "speed": 0.15,
        "intensity": 0.48,
        "dimensionality": 4.0 /* Conceptual */
      }
    },
    "category-vault-visual": {
      "description": "Visual theme for Visualizer Vault category.",
      "parameters": {
        "shape": "crystal",
        "baseColor": [0.7, 0.7, 0.9, 0.2], /* More prominent for showcases */
        "speed": 0.05, /* Slower, to appreciate the form */
        "intensity": 0.65,
        "reflectivity": 0.7 /* Conceptual */
      }
    },
    "category-audio-visual": {
      "description": "Visual theme for Audio Waves category.",
      "parameters": {
        "shape": "wave",
        "baseColor": [0.5, 0.9, 0.5, 0.12],
        "speed": 0.25, /* Active, flowing */
        "intensity": 0.4,
        "amplitude": 0.6 /* Conceptual */
      }
    },
    "category-news-visual": {
      "description": "Visual theme for Communiques (News) category.",
      "parameters": {
        "shape": "hypercube",
        "baseColor": [0.9, 0.9, 0.9, 0.1], /* Cleaner, brighter */
        "speed": 0.12,
        "intensity": 0.35,
        "pulseFrequency": 0.5 /* Conceptual */
      }
    },
    "article-header-visual": {
        "description": "Subtle visual for article headers.",
        "parameters": {
            "shape": "wave",
            "color": [0.4, 0.4, 0.5, 0.05],
            "speed": 0.02,
            "intensity": 0.1,
            "heightPercentage": 0.2 /* Conceptual: only fill 20% of container height */
        }
    }
  },
  "interactionPresets": {
    "glass-panel-hover": {
      "description": "Hover effect for glassmorphic panels.",
      "events": ["mouseover", "mouseout"],
      "targetActions": {
        "mouseover": {
          "duration": 200,
          "params": {
            "intensity": 0.05, /* Slight increase in internal pattern intensity */
            "edgeHighlightColor": [0.5, 0.8, 1.0, 0.7] /* Make edge glow visible */
          }
        },
        "mouseout": {
          "duration": 400,
          "resetToBase": true
        }
      },
      "ecosystemReactions": []
    },
    "category-link-pulse": {
        "description": "Pulse effect for category navigation links on hover.",
        "events": ["mouseover", "mouseout"],
        "targetActions": {
            "mouseover": {
                "duration": 150,
                "params": {"intensity": 0.8, "scale": 1.1} /* Assuming VIB34D instance on link */
            },
            "mouseout": {
                "duration": 300,
                "resetToBase": true
            }
        },
        "ecosystemReactions": []
    },
    "featured-article-focus": {
        "description": "Interaction for focusing on a featured article.",
        "events": ["click"], /* Could be scroll-based later */
        "targetActions": {
            "click": {
                "duration": 500,
                "params": {"intensity": 1.0, "speed": 0.01, "zoomFactor": 1.2} /* Conceptual zoom */
            }
        },
        "ecosystemReactions": [
            {
                "selector": ".article-card:not(.featured)", /* Example for other cards */
                "excludeTarget": true,
                "actions": {
                    "click": {
                        "duration": 500,
                        "params": { "intensity": 0.1, "speed": 0.3, "blurFactor": 0.5 } /* Conceptual blur */
                    }
                }
            }
        ]
    }
  }
}
